{
  "name": "zodiac-module-oz-governor",
  "version": "1.1.0",
  "description": "Out of the box starting point for building Zodiac modules",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnosisguild/zodiac-module-oz-governor.git"
  },
  "scripts": {
    "build": "hardhat compile",
    "coverage": "hardhat coverage",
    "deploy": "hardhat deploy",
    "reconstruct:mastercopy": "yarn run build && yarn hardhat mastercopy:reconstruct --network",
    "extract:mastercopy": "yarn run build && yarn hardhat extract:mastercopy",
    "deploy:mastercopies": "yarn hardhat deploy:mastercopies --network",
    "deploy:mastercopy": "yarn hardhat deploy:mastercopy --network",
    "verify:mastercopies": "yarn hardhat verify:mastercopies --network",
    "verify:mastercopy": "yarn hardhat verify:mastercopy --network",
    "test": "hardhat test",
    "lint": "yarn lint:sol && yarn lint:ts",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:ts": "eslint src test --ext .ts",
    "fmt": "yarn fmt:sol && yarn fmt:ts",
    "fmt:sol": "prettier 'contracts/**/*.sol' -w",
    "fmt:ts": "prettier '(test|src)/**/*.ts' -w",
    "size": "hardhat size-contracts"
  },
  "keywords": [
    "zodiac",
    "gnosis-safe",
    "hardhat"
  ],
  "author": "Gnosis Guild / dOrg",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/gnosis/zodiac-module-oz-governor/issues"
  },
  "homepage": "https://github.com/gnosis/zodiac-module-oz-governor",
  "devDependencies": {
    "@gnosis-guild/zodiac-core": "^2.0.5",
    "@gnosis.pm/safe-contracts": "^1.3.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.7",
    "@nomicfoundation/hardhat-ethers": "^3.0.6",
    "@nomicfoundation/hardhat-ignition": "^0.15.5",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.11",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.9",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.5.6",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "chai": "^4.3.6",
    "dotenv": "^16.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "ethers": "^6.13.2",
    "hardhat": "2.22.7",
    "hardhat-gas-reporter": "^2.2.0",
    "prettier": "^3.3.3",
    "prettier-plugin-solidity": "^1.4.1",
    "solhint": "5.0.3",
    "solhint-plugin-prettier": "0.1.0",
    "solidity-coverage": "^0.8.12",
    "ts-node": "^10.7.0",
    "typechain": "^8.1.1",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.5.0",
    "@openzeppelin/contracts-upgradeable": "4.9.3",
    "hardhat-contract-sizer": "^2.6.1"
  }
}
